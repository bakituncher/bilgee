      "id": "lgs_1",
      "topic": "Üslü Sayılar",
      "subject": "Matematik",
      "label": "Üslü Sayılar",
      "description": "Üslü ifadelerin tanımı ve işlemleri",
      "children": [
        {
          "label": "Üs Kavramı",
          "description": "Bir sayının kendisiyle tekrarlı çarpımı",
          "children": [
            {
              "label": "Pozitif Üs",
              "description": "$a^n = a \\times a \\times ... \\times a$ (n kez)"
            },
            {
              "label": "Negatif Üs",
              "description": "$a^{-n} = \\frac{1}{a^n}$"
            },
            {
              "label": "Sıfır Üssü",
              "description": "$a^0 = 1$ ($a \\neq 0$)"
            }
          ]
        },
        {
          "label": "Üslü Sayılarda İşlemler",
          "description": "Üslü ifadelerin toplama, çarpma, bölme kuralları",
          "children": [
            {
              "label": "Çarpma",
              "description": "$a^m \\times a^n = a^{m+n}$"
            },
            {
              "label": "Bölme",
              "description": "$a^m \\div a^n = a^{m-n}$"
            },
            {
              "label": "Üssün Üssü",
              "description": "$(a^m)^n = a^{m \\times n}$"
            }
          ]
        },
        {
          "label": "Özel Durumlar",
          "description": "Üslü sayılarda dikkat edilmesi gerekenler",
          "children": [
            {
              "label": "Çarpımın Üssü",
              "description": "$(a \\times b)^n = a^n \\times b^n$"
            },
            {
              "label": "Bölümün Üssü",
              "description": "$(a \\div b)^n = a^n \\div b^n$"
            },
            {
              "label": "Kesirli Üs",
              "description": "$a^{m/n} = \\sqrt[n]{a^m}$"
            }
          ]
        },
        {
          "label": "Problem Çözme",
          "description": "Üslü sayılarla ilgili soru tipleri",
          "children": [
            {
              "label": "Eşitleme",
              "description": "Tabanları veya üsleri eşitleyerek çözme"
            },
            {
              "label": "Sadeleştirme",
              "description": "Karmaşık üslü ifadeleri basitleştirme"
            }
          ]
        }
      ]
    },
