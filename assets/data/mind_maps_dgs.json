      "id": "dgs_mat_1",
      "topic": "Temel Kavramlar (Sayılar)",
      "subject": "Matematik",
      "label": "Temel Kavramlar (Sayılar)",
      "description": "Doğal sayılar, tam sayılar ve rasyonel sayılar",
      "children": [
        {
          "label": "Doğal Sayılar",
          "description": "Sıfır ve pozitif tam sayılar: $\\mathbb{N} = \\{0, 1, 2, 3, ...\\}$"
        },
        {
          "label": "Tam Sayılar",
          "description": "Pozitif, negatif ve sıfır: $\\mathbb{Z} = \\{..., -2, -1, 0, 1, 2, ...\\}$"
        },
        {
          "label": "Rasyonel Sayılar",
          "description": "Kesirli sayılar: $\\mathbb{Q} = \\{\\frac{a}{b} | a, b \\in \\mathbb{Z}, b \\neq 0\\}$"
        }
      ]
    },
    {
      "id": "dgs_mat_2",
      "topic": "Üslü ve Köklü Sayılar",
      "subject": "Matematik",
      "label": "Üslü ve Köklü Sayılar",
      "description": "Üslü ve köklü ifadelerin özellikleri",
      "children": [
        {
          "label": "Üslü Sayılar",
          "description": "$a^m \\times a^n = a^{m+n}$",
          "children": [
            {
              "label": "Çarpma",
              "description": "$a^m \\times a^n = a^{m+n}$"
            },
            {
              "label": "Bölme",
              "description": "$\\frac{a^m}{a^n} = a^{m-n}$"
            }
          ]
        },
        {
          "label": "Köklü Sayılar",
          "description": "$\\sqrt[n]{a^m} = a^{\\frac{m}{n}}$",
          "children": [
            {
              "label": "Çarpma",
              "description": "$\\sqrt[n]{a} \\times \\sqrt[n]{b} = \\sqrt[n]{a \\times b}$"
            },
            {
              "label": "Bölme",
              "description": "$\\frac{\\sqrt[n]{a}}{\\sqrt[n]{b}} = \\sqrt[n]{\\frac{a}{b}}$"
            }
          ]
        }
      ]
    },
    {
      "id": "dgs_mat_3",
      "topic": "Kombinasyon ve Permütasyon",
      "subject": "Matematik",
      "label": "Kombinasyon ve Permütasyon",
      "description": "Seçme ve sıralama yöntemleri",
      "children": [
        {
          "label": "Permütasyon",
          "description": "Sıralama önemli: $P(n,r) = \\frac{n!}{(n-r)!}$"
        },
        {
          "label": "Kombinasyon",
          "description": "Seçim önemli: $C(n,r) = \\frac{n!}{r!(n-r)!}$"
        },
        {
          "label": "Faktöriyel",
          "description": "$n! = n \\times (n-1) \\times ... \\times 2 \\times 1$"
        }
      ]
    },
    {
      "id": "dgs_mat_4",
      "topic": "Olasılık",
      "subject": "Matematik",
      "label": "Olasılık",
      "description": "Olayların gerçekleşme ihtimali",
      "children": [
        {
          "label": "Temel Olasılık",
          "description": "$P(A) = \\frac{\\text{İstenen Durum}}{\\text{Tüm Durumlar}}$"
        },
        {
          "label": "Birleşim Olasılığı",
          "description": "$P(A \\cup B) = P(A) + P(B) - P(A \\cap B)$"
        },
        {
          "label": "Bağımsız Olaylar",
          "description": "$P(A \\cap B) = P(A) \\times P(B)$"
        }
      ]
    },
